{
  "name": "@afetcan/api",
  "private": false,
  "type": "module",
  "version": "0.0.12",
  "description": "Api for afetcan.com",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "Apache",
  "homepage": "https://github.com/afetcan/#readme",
  "packageManager": "pnpm@7.26.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afetcan/api.git"
  },
  "bugs": {
    "url": "https://github.com/afetcan/api/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.26.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "graphql:generate": "graphql-codegen",
    "generate": "pnpm graphql:generate"
  },
  "peerDependencies": {
    "@abraham/reflection": "^0.12.0",
    "graphql": "16.6.0"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@afetcan/storage": "^0.0.26",
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/s3-request-presigner": "^3.282.0",
    "@clickhouse/client": "^0.0.12",
    "@envelop/generic-auth": "^6.1.1",
    "@envelop/graphql-modules": "^5.0.3",
    "@envelop/response-cache-redis": "^3.3.2",
    "@graphql-tools/links": "^9.0.0",
    "@graphql-tools/resolvers-composition": "^7.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@huntersofbook/relay-cursor-paging": "^1.0.0",
    "blurhash": "^2.0.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "fastify": "^4.13.0",
    "geojson": "^0.5.0",
    "graphql": "16.6.0",
    "graphql-modules": "^2.1.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "^4.0.5",
    "ioredis": "^5.3.1",
    "merge-anything": "^5.1.4",
    "nanoid": "^5.0.1",
    "pathe": "^1.1.0",
    "sharp": "^0.31.3",
    "supertokens-node": "^16.2.1",
    "uuid": "^9.0.0",
    "wait-on": "^7.0.1",
    "wkx": "^0.5.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/graphql-modules-preset": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "graphql-codegen-typescript-validation-schema": "^0.7.1",
    "@faker-js/faker": "^8.1.0",
    "@types/node": "^18.14.2",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@types/wait-on": "^5.3.1",
    "bumpp": "^9.0.0",
    "consola": "^3.2.3",
    "eslint": "^8.35.0",
    "tslib": "^2.5.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "pnpm": {
    "overrides": {
      "graphql": "16.6.0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "graphql": "16.6.0"
      }
    }
  },
  "resolutions": {
    "graphql": "16.6.0"
  }
}
