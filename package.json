{
  "name": "@afetcan/api",
  "private": false,
  "type": "module",
  "version": "0.0.12",
  "description": "Api for afetcan.com",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "Apache",
  "homepage": "https://github.com/afetcan/#readme",
  "packageManager": "pnpm@7.26.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afetcan/api.git"
  },
  "bugs": {
    "url": "https://github.com/afetcan/api/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.26.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "graphql:generate": "graphql-codegen",
    "generate": "pnpm graphql:generate"
  },
  "peerDependencies": {
    "@abraham/reflection": "^0.12.0",
    "graphql": "16.8.1"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@afetcan/storage": "^0.0.26",
    "@aws-sdk/client-s3": "^3.425.0",
    "@aws-sdk/s3-request-presigner": "^3.425.0",
    "@clickhouse/client": "^0.2.2",
    "@envelop/generic-auth": "^5.0.6",
    "@envelop/graphql-modules": "^4.0.6",
    "@envelop/response-cache-redis": "^2.0.8",
    "@graphql-tools/links": "^8.3.36",
    "@graphql-tools/resolvers-composition": "^6.5.18",
    "@graphql-tools/schema": "^9.0.19",
    "@huntersofbook/relay-cursor-paging": "^1.0.0",
    "blurhash": "^2.0.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "geojson": "^0.5.0",
    "graphql": "16.8.1",
    "graphql-modules": "^2.2.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.22.2",
    "graphql-yoga": "^3.9.1",
    "ioredis": "^5.3.2",
    "merge-anything": "^5.1.7",
    "nanoid": "^4.0.2",
    "pathe": "^1.1.1",
    "sharp": "^0.32.6",
    "supertokens-node": "^13.6.1",
    "uuid": "^9.0.1",
    "wait-on": "^7.0.1",
    "wkx": "^0.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/graphql-modules-preset": "^3.1.3",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-codegen/typescript-resolvers": "^3.2.1",
    "graphql-codegen-typescript-validation-schema": "^0.12.0",
    "@faker-js/faker": "^7.6.0",
    "@types/node": "^18.18.3",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.4",
    "@types/wait-on": "^5.3.2",
    "bumpp": "^9.2.0",
    "consola": "^2.15.3",
    "eslint": "^8.50.0",
    "tslib": "^2.6.2",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "pnpm": {
    "overrides": {
      "graphql": "16.6.0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "graphql": "16.6.0"
      }
    }
  },
  "resolutions": {
    "graphql": "16.8.1"
  }
}
